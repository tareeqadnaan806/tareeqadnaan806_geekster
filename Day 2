        Scanner sc=new Scanner(System.in);
            double length=sc.nextDouble();
            double breadth=sc.nextDouble();
            System.out.println(length*breadth);
                }
    }



Q3)
    public class Main {
        public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
            int x=sc.nextInt();
            boolean ans=x>1000;
            System.out.println(ans);
                }
    }



